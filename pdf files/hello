      "source": [
        "def reformateTime(time):\n",
        "  time_list = list(time)\n",
        "  for ind in range(len(time_list)):\n",
        "    if time_list[ind] == \"-\":\n",
        "      time_list[ind] = '/'\n",
        "    elif time_list[ind] == \"T\":\n",
        "      return ''.join(time_list[2:ind])\n",
        "      print(time_list)\n",
        "  return ''.join(time_list)\n",
        "\n"
      ],
      "execution_count": null,
      "outputs": []
    },
