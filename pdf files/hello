        "# return closest previous date in the list of dates\n",
        "def findDate(date_list_a, time):\n",
        "  now = dt.strptime('22/11/15', \"%y/%m/%d\")\n",
        "  min = now-time\n",
        "\n",
        "  for item in date_list_a:\n",
        "    if time-item < min:\n",
        "      max = item\n",
        "  \n",
        "  return date_list_a.index(max)\n"
      ],
